{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "quotes": "off",
    "no-shadow": "off",
    "newline-after-import" :"off",
    "no-console": "off",
    "comma-dangle":"off",
    "no-underscore-dangle":"off"
  }
}
